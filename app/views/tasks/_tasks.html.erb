<ul class="media-list">
  
<% @tasks.each do |task| %>

    <li class="media">
      
      <div class="media-body">
        <div>
 <span class="text-muted">posted at 
<%= task.created_at %>
</span>
        </div>
        <div>
          <p><%= task.content %></p>
          <p><%= task.status %></p>
        </div>
      </div>
    </li>
  
<% end %>

  
<%= paginate @tasks %>

</ul>
